{
  "trailingComma": "es5",
  "useTabs": false,
  "printWidth": 80,
  "jsxBracketSameLine": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": "src/**/*.js",
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    }
  ]
}
